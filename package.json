{
  "name": "@samhammer/ckeditor5-internallink-plugin",
  "version": "1.0.13",
  "description": "A generic ckeditor plugin to create link tags containing an internal identifier instead of a url.",
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "ckeditor5-feature"
  ],
  "dependencies": {
    "@ckeditor/ckeditor5-core": "^36.0.1",
    "@ckeditor/ckeditor5-engine": "^29.0.0",
    "@ckeditor/ckeditor5-ui": "^36.0.1",
    "awesomplete": "^1.1.2",
    "axios": "^0.21.4"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-editor-classic": "^36.0.1",
    "@ckeditor/ckeditor5-enter": "^36.0.1",
    "@ckeditor/ckeditor5-heading": "^36.0.1",
    "@ckeditor/ckeditor5-paragraph": "^36.0.1",
    "@ckeditor/ckeditor5-typing": "^36.0.1",
    "@ckeditor/ckeditor5-undo": "^36.0.1",
    "@ckeditor/ckeditor5-utils": "^29.0.0",
    "eslint": "^5.9.0",
    "eslint-config-ckeditor5": "^1.0.7",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0"
  },
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.0.0"
  },
  "files": [
    "lang",
    "src",
    "theme"
  ],
  "scripts": {
    "lint": "eslint --quiet src/**/*.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --quiet"
    ]
  },
  "eslintIgnore": [
    "src/lib/**",
    "packages/**"
  ]
}
